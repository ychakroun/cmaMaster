{% extends "homePageBundle::base.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block javascripts %}
    <script src="{{asset( "js/tags.js" )}}"></script>
     <script src="{{asset( "js/imgProfileEdit.js" )}}"></script>
    {{parent()}}
{% endblock %}
{% block main %}
{% include "homePageBundle:Profile:edit.html.twig" %}
{% endblock main %}
{% block script %}
    <script >
    {% if profile is null  %}
    {% else %}

		{% if profile.imageHeader is null  %}
		{% else %}
			changeHeader('{{asset( userServices.getImageHeader )}}');
		{% endif %}
    {% endif %}
    setAddContent('{{ "form.profile.tag.add" | trans }}');
 </script>
{% endblock %}


 
